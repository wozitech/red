{
  "parser": "esprima",
  "extends": [
    "eslint:recommended",
    "prettier",
    "strongloop"
  ],
  "rules": {
    "comma-dangle": 0,
    "import/order": [
      "error", {
        "groups": [
        "builtin",
        "external",
        "internal",
        "parent",
        "sibling",
        "index"
      ]
      }
    ],
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "wyze/sort-destructuring-keys": 0,
    "arrow-parens": 0,
    "max-len": [
      "error",
      120
    ],
    "arrow-body-style": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ],
    "newline-per-chained-call": 0,
    "import/prefer-default-export": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 1,
    "import/no-named-as-default": 0,
    "react/no-danger": 0
  },
  "globals": {
  },
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "jest",
    "wyze",
    "import"
  ]
}
